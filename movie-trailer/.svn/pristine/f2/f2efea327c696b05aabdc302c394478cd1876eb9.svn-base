package com.samsung.trailer.vo;

import org.codehaus.jackson.annotate.JsonIgnoreProperties;
import org.springframework.data.annotation.Id;

@JsonIgnoreProperties({"_id"})
public class BaseVO {
	@Id
	public Long id;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}
	
}
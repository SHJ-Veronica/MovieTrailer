<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:dwr="http://www.directwebremoting.org/schema/spring-dwr"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
						   http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
						   http://www.directwebremoting.org/schema/spring-dwr
						   http://www.directwebremoting.org/schema/spring-dwr-3.0.xsd">

	<!-- DWR -->
	<dwr:annotation-config base-package="com.samsung.trailer" scanDataTransferObject="true" scanRemoteProxy="true"/>
	<dwr:controller id="dwrController" debug="true" />

	<dwr:configuration>
		<dwr:convert class="java.lang.Object" type="bean"  />
		<dwr:convert type="bean" class="com.samsung.trailer.vo.Trailer"/>
 	</dwr:configuration>

</beans>